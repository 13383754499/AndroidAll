<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.module.chiclaim.com.fragment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.module.chiclaim.com.fragment.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.module.chiclaim.com.fragment.FragmentLifecycleActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.module.chiclaim.com.fragment.ResultActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.module.chiclaim.com.fragment.ActivityTestFragmentResult"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.module.chiclaim.com.fragment.ViewPagerActivity"
            android:label="ViewPager"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.module.chiclaim.com.fragment.DrawerLayoutActivity"
            android:label="DrawerLayout"
            android:screenOrientation="portrait" />
    </application>

</manifest>